<script>
  document.addEventListener('DOMContentLoaded', function () {
    const logoutBtn = document.getElementById('logoutBtn');

    // Check if the button exists
    if (logoutBtn) {
      logoutBtn.addEventListener('click', async function (event) {
        event.preventDefault(); // Prevent default button action
        alert('Logout process initiated.'); // Alert when logout is initiated

        // Send the POST request to the logout route
        try {
          const response = await fetch('/logout', {
            method: 'POST',
            headers: {
              'Content-Type': 'application/json'
            },
            body: JSON.stringify({ Token: '' }) // Send token or leave it empty for cookie-based logout
          });

          if (response.ok) {
            alert('Logout successful! Redirecting to the login page.'); // Alert on successful logout

            // Attempt redirection to the primary URL
            fetch('/loginpage', { method: 'HEAD' })
              .then((res) => {
                if (res.ok) {
                  // Redirect if the primary URL is accessible
                  window.location.href = '/loginpage';
                } else {
                  throw new Error("Primary login page unavailable");
                }
              })
              .catch(() => {
                // Fallback to localhost if the primary URL fails
                console.error("Failed to load the login page, redirecting to localhost.");
                window.location.href = '/loginpage';
              });
          }
          else {
            alert('Failed to log out. Please try again.'); // Alert on failed logout
            console.error('Failed to log out');
          }
        } catch (error) {
          alert('Error during logout: ' + error.message); // Alert on error during fetch
          console.error('Error during logout:', error);
        }
      });
    } else {
      console.error('Logout button not found'); // Log if the button is not found
    }
  });
</script>


