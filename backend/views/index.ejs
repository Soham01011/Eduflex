  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.9.3/Chart.min.css" />
  <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.9.3/Chart.min.js" defer></script>
  <script src="/assets/js/charts-lines.js" defer></script>
  <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>


  <script>
    // Safely inject pointsData from server-side
    const pointsData = <%- JSON.stringify(pointsData) %>;
  
    // Define categories with initial point totals
    const categories = {
      academic: 0,
      extra: 0,
      experience: 0,
    };
  
    // Aggregate points by category
    pointsData.forEach((entry) => {
      if (entry.post_type in categories) {
        categories[entry.post_type] += entry.points;
      }
    });
  
    // Create the doughnut chart
    const createDoughnutChart = () => {
      const ctx = document.getElementById('doughnutChart').getContext('2d');
      const chart = new Chart(ctx, {
        type: 'doughnut',
        data: {
          labels: Object.keys(categories), // 'academic', 'extracurricular', 'experience'
          datasets: [{
            label: 'Points by Category',
            data: Object.values(categories), // Corresponding points for each category
            backgroundColor: ['#FF6384', '#36A2EB', '#FFCE56'], // You can customize the colors
            hoverOffset: 4
          }]
        },
        options: {
          responsive: true,
          plugins: {
            legend: {
              position: 'top',
            },
            tooltip: {
              callbacks: {
                label: function (tooltipItem) {
                  return tooltipItem.label + ': ' + tooltipItem.raw + ' points';
                }
              }
            }
          }
        }
      });
    };
  
    // Update card values dynamically
    const updateCardValues = () => {
      Object.entries(categories).forEach(([postType, points]) => {
        const cardElement = document.querySelector(`[data-post-type="${postType}"] .text-lg`);
        if (cardElement) {
          cardElement.textContent = points;
        }
      });
  
      // Create or update the doughnut chart after DOM is fully loaded
      createDoughnutChart();
    };
  
    // Trigger updates after DOM is fully loaded
    document.addEventListener('DOMContentLoaded', updateCardValues);
  </script>
  
